# GO Commerce Administration Console - Environment Configuration
# Copy this file to .env.development for local development
# Copy this file to .env.production for production deployment

# Application Configuration
VITE_APP_NAME="GO Commerce Admin Console"
VITE_APP_VERSION="1.0.0"
VITE_APP_DESCRIPTION="Administration console for GO Commerce multi-tenant e-commerce platform"

# API Configuration
VITE_API_BASE_URL=http://localhost:8080/api/v1
VITE_API_TIMEOUT=10000

# Keycloak Authentication Configuration
VITE_KEYCLOAK_URL=http://localhost:9000
VITE_KEYCLOAK_REALM=gocommerce
VITE_KEYCLOAK_CLIENT_ID=gocommerce-admin-console

# WebSocket Configuration (for real-time features)
VITE_WEBSOCKET_URL=ws://localhost:8080/ws

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REAL_TIME_UPDATES=true
VITE_ENABLE_EXPORT_FEATURES=true
VITE_ENABLE_DEBUG_MODE=false

# UI Configuration
VITE_DEFAULT_LOCALE=en
VITE_DEFAULT_TIMEZONE=UTC
VITE_DEFAULT_CURRENCY=USD

# Development Configuration
VITE_ENABLE_MOCK_DATA=false
VITE_LOG_LEVEL=info

# Security Configuration
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=false

# Performance Configuration
VITE_ENABLE_SERVICE_WORKER=true
VITE_CACHE_DURATION=300000
