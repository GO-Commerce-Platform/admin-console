# GO Commerce Administration Console - Development Environment
# This file contains environment variables for local development

# Application Configuration
VITE_APP_NAME="GO Commerce Admin Console (Development)"
VITE_APP_VERSION="1.0.0-dev"
VITE_APP_DESCRIPTION="Administration console for GO Commerce multi-tenant e-commerce platform"

# API Configuration
VITE_API_BASE_URL=http://localhost:8080/api/v1
VITE_API_TIMEOUT=10000

# Keycloak Authentication Configuration
VITE_KEYCLOAK_URL=http://localhost:9001
VITE_KEYCLOAK_DIRECT_URL=http://localhost:9000
VITE_KEYCLOAK_REALM=gocommerce
VITE_KEYCLOAK_CLIENT_ID=gocommerce-admin-console

# WebSocket Configuration (for real-time features)
VITE_WEBSOCKET_URL=ws://localhost:8080/ws

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REAL_TIME_UPDATES=false
VITE_ENABLE_EXPORT_FEATURES=true
VITE_ENABLE_DEBUG_MODE=true

# UI Configuration
VITE_DEFAULT_LOCALE=en
VITE_DEFAULT_TIMEZONE=UTC
VITE_DEFAULT_CURRENCY=USD

# Development Configuration
VITE_ENABLE_MOCK_DATA=true
VITE_LOG_LEVEL=debug

# Security Configuration
VITE_ENABLE_CSP=false
VITE_ENABLE_HTTPS_ONLY=false

# Performance Configuration
VITE_ENABLE_SERVICE_WORKER=false
VITE_CACHE_DURATION=0
