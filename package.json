{
  "name": "gocommerce-admin-console",
  "displayName": "GO Commerce Administration Console",
  "description": "Modern Vue.js administration console for GO Commerce multi-tenant e-commerce platform",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "author": "GO Commerce Team",
  "license": "PROPRIETARY",
  "homepage": "https://github.com/gocommerce/admin-console",
  "repository": {
    "type": "git",
    "url": "https://github.com/gocommerce/admin-console.git"
  },
  "keywords": [
    "vue",
    "typescript",
    "admin",
    "ecommerce",
    "multi-tenant",
    "dashboard",
    "pinia",
    "naive-ui"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --mode development",
    "dev:debug": "vite --mode development --debug",
    "build": "npm run type-check && vite build",
    "build:analyze": "npm run build -- --analyze",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "preview": "vite preview",
    "preview:build": "npm run build && npm run preview",
    "type-check": "vue-tsc --noEmit --composite false",
    "type-check:watch": "vue-tsc --noEmit --watch",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "format": "prettier --write \"src/**/*.{vue,js,ts,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{vue,js,ts,json,css,scss,md}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "clean": "rm -rf dist node_modules/.cache .eslintcache .prettiercache",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "prepare": "npm run type-check && npm run lint:check && npm run format:check",
    "ci": "npm ci && npm run prepare && npm run test:run && npm run build",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@vicons/tabler": "^0.13.0",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "keycloak-js": "^26.2.0",
    "naive-ui": "^2.43.1",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.4.0",
    "jsdom": "^26.1.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  }
}
